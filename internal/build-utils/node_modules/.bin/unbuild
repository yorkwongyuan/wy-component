#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wangy/IT/vue/components-lib/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/wangy/IT/vue/components-lib/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/unbuild@1.2.1/node_modules/unbuild/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/unbuild@1.2.1/node_modules/unbuild/dist/cli.mjs" "$@"
fi
